{
    "name": "mougrim/php-logger",
    "description": "Simple and fast php logging library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mitallast",
            "email": "mitallast@gmail.com"
        },
        {
            "name": "Mougrim",
            "email": "rinat@mougrim.ru"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "phpunit/phpunit": "dev-4.8.27-soft-mocks",
        "phpunit/php-code-coverage": "dev-2.2.4-soft-mocks",
        "badoo/soft-mocks": "dev-forLogger",
        "johnkary/phpunit-speedtrap": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mougrim\\Logger\\": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:mougrim/soft-mocks.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:mougrim/phpunit.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:mougrim/php-code-coverage.git"
        }
    ]
}
